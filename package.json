{
  "name": "tzx",
  "version": "0.1.5",
  "description": "Easily execute zx scripts written in typescript",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "repository": "https://github.com/realfresh/tzx",
  "type": "module",
  "tsup": {
    "target": ["node16"],
    "format": "esm",
    "sourcemap": true,
    "clean": true
  },
  "scripts": {
    "link": "npm link",
    "watch": "tsup-node --watch ./src/index.ts",
    "build": "tsup-node ./src/index.ts",
    "pub": "git add . && git commit -am . ; npm publish",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "zx": "7.0.8"
  },
  "devDependencies": {
    "tsup": "^6.2.3",
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "prettier": "^2.7.1"
  },
  "author": "",
  "license": "ISC"
}
